
<?xml version="1.0" encoding="ISO-8859-1" ?>

<skin version="1.0.0">

	<layout>

		<image src="background.jpg" x="0" y="0"></image>

		<visualization width="210" height="74" x="2" y="64" color="0xFF9dfe00" clickList="fft, peak">
			<fft src="fft.png" blur="0"></fft>
			<peak src="peak.png" blur="0"></peak>
		</visualization>

		<image src="button_bg.png" x="5" y="101"></image>

		<font id="TEXT" 	charW="8"  charH="8" chars="ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789!@#$%^&amp;*()-_+=[]{}|\/:;,.&lt;&gt;?' " src="font.png"></font>
		<font id="DIGITS" 	charW="15" charH="21" chars="1234567890:" src="digits.png"></font>

		<button id="PLAY" 	frameH="34" x="93" y="125" src="b_play.png"></button>
		<button id="STOP" 	frameH="26" x="131" y="111" src="b_stop.png"></button>
		<button id="NEXT" 	frameH="26" x="131" y="154" src="b_next.png"></button>
		<button id="PAUSE" 	frameH="26" x="68"  y="111" src="b_pause.png"></button>
		<button id="PREV" 	frameH="26" x="68"  y="154" src="b_prev.png"></button>

		<display id="SONGINFO" width="195" height="8"  x="12" y="51" font="TEXT"    defaultText="E-PHONIC MP3-PLAYER - GET YOUR OWN FREE SKINNABLE MP3-PLAYER AT WWW.E-PHONIC.COM/MP3PLAYER"></display>
		<display id="SONGTIME" width="200" height="20" x="11" y="23" font="DIGITS"  defaultText="00:00"></display>

		<slider id="TRANSPORT" x="90" y="28">
			<background src="transport_bg.png"></background>
			<handle src="transport_handle.png" frameH="12"></handle>
			<indicator src="transport_loaded.png"></indicator>
		</slider>

		<slider id="VOLUME" x="155" y="140">
			<background src="volume_bg.png"></background>
			<handle src="transport_handle.png" frameH="12"></handle>
			<indicator src="volume_indicator.png"></indicator>
		</slider>

		<playlist width="255" height="66" x="2" y="193" font="TEXT">
			<button frameH="11" txtX="8" txtY="2" src="b_playlist.png"></button>
		</playlist>

		<button id="PLAYLIST_UP" 	frameH="19" x="190" y="221" 	src="b_pl_up.png"></button>
		<button id="PLAYLIST_DOWN" 	frameH="20" x="190" y="240" 	src="b_pl_down.png"></button>

		<ephonic_icon x="15" y="140"></ephonic_icon>

	</layout>

</skin>