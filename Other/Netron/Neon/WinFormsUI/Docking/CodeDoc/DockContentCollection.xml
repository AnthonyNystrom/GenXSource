<?xml version="1.0" encoding="utf-8" ?>
<CodeDoc>
	<Class name="DockContentCollection">
		<ClassDef>
			<summary>Provides methods to manage a collection of <see cref="Netron.Neon.WinFormsUI.IDockContent" /> objects.</summary>
		</ClassDef>
		<Property name="Item">
			<summary>
			<para>Gets a <see cref="Netron.Neon.WinFormsUI.IDockContent" /> at the specified index.</para>
			<para>In C#, this property is the indexer for the <see cref="Netron.Neon.WinFormsUI.DockContentCollection" /> class.</para>
			</summary>
		</Property>
		<Method name="Contains(IDockContent)">
			<summary>
			Determines whether a <see cref="Netron.Neon.WinFormsUI.IDockContent" /> is in the
			<see cref="Netron.Neon.WinFormsUI.DockContentCollection" />.
		</summary>
			<param name="content">
			The <see cref="Netron.Neon.WinFormsUI.IDockContent" /> to locate in
			<see cref="Netron.Neon.WinFormsUI.DockContentCollection" />.
		</param>
			<returns>
			<b>true</b> if <paramref name="content" /> is found in the
			<see cref="Netron.Neon.WinFormsUI.DockContentCollection" />;
			otherwise, <b>false</b>.
		</returns>
		</Method>
		<Method name="IndexOf(IDockContent)">
			<summary>
			Searches for the specified <see cref="Netron.Neon.WinFormsUI.IDockContent" />
			and returns the zero-based index within the entire
			<see cref="Netron.Neon.WinFormsUI.DockContentCollection" />.
		</summary>
			<param name="content">
			The <see cref="Netron.Neon.WinFormsUI.IDockContent" />
			to locate in the <see cref="Netron.Neon.WinFormsUI.DockContentCollection" />
		</param>
			<returns>
			The zero-based index of <paramref name="content" /> within the entire
			<see cref="Netron.Neon.WinFormsUI.DockContentCollection" />, if found;
			otherwise, -1.
		</returns>
		</Method>
		<Method name="Select(DockState)"></Method>
	</Class>
</CodeDoc>
